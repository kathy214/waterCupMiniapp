.user{
  height: 260rpx;
  text-align: center;
  position: relative;
}
.user image{
  width: 100%;
}
.user .text{
  width: 100%;
  position: absolute;
  top: 50rpx;
  left: 0;
  text-align: center;
  color: 40rpx;
  color: #FFFFFF;
}
.other{
  background: #FFFFFF;
  border-radius: 40rpx;
  margin: -80rpx 30rpx 0;
  padding: 0 50rpx;
  position: relative;
}
.other .item{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 110rpx;
  border-bottom: 1px solid rgba(0,0,0,0.1);
}
.other .item:last-child{
  border: none;
}
.other .item .label{
  display: flex;
  align-items: center;
  font-weight: 500;
  font-size: 32rpx;
  color: #000000;
}
.other .item .label image{
  width: 60rpx;
  margin-right: 16rpx;
}
.other .item .right-btn{
  width: 36rpx;
}
.bottom{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  padding: 0 110rpx 80rpx;
  box-sizing: border-box;
}
.bottom .btn{
  height: 84rpx;
  line-height: 84rpx;
  text-align: center;
  background: #32BAC0;
  border-radius: 200rpx;
  font-size: 34rpx;
  color: #FFFFFF;
}