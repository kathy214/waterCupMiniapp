.main{
  background: #F2F3F5;
  height: 100vh;
}
.show-content{
  width: 100%;
  height: 680rpx;
  text-align: center;
}
.show-content .bg{
  width: 100%;
}
.show-content .fingerprint{
  position: absolute;
  width: 466rpx;
  left: 50%;
  top: 64rpx;
  margin-left: -233rpx;
}
.mian-content{
  padding: 0 30rpx;
  margin-top: -86rpx;
  position: relative;
}
.mian-content .state{
  padding:46rpx 0 46rpx 40rpx;
  display: flex;
  align-items: center;
}
.mian-content .state .item{
  display: flex;
  align-items: center;
}
.mian-content .state .line{
  height: 48rpx;
  border-right: 1px solid #E5E5E5;
  margin-left: 88rpx;
  margin-right: 40rpx;
}
.mian-content .state .item image{
  width: 84rpx;
  margin-right: 12rpx;
}
.mian-content .state .item .describe text{
  font-size: 28rpx;
  color: rgba(0,0,0,0.6);
}
.mian-content .state .item .describe .value{
  font-weight: 500;
  font-size: 40rpx;
  color: #32BAC0;
  margin-top: 12rpx;
}
.mian-content .fingerprint-list{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 40rpx;
  height: 160rpx;
}
.mian-content .fingerprint-list .right{
  font-size: 28rpx;
  color: #32BAC0;
  display: flex;
}
.mian-content .fingerprint-list .right image{
  width: 36rpx;
  margin-left: 20rpx;
}
.bottom{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  background: #ffffff;
  padding: 20rpx 116rpx 60rpx;
}
.bottom .button{
  width: 520rpx;
  height: 84rpx;
  line-height: 84rpx;
  text-align: center;
  font-size: 32rpx;
  color: #FFFFFF;
  background: #32BAC0;
  border-radius: 120rpx;
}
